<h1>新規投稿</h1>

<%= form_with model: @book do |f| %>

  <h4>title</h4>
  <%= f.text_field :title%>

  <h4>opinion</h4>
  <%= f.text_area :body%>

  <div>
    <%= f.submit 'update' %>
  </div>

  <div>
    <%= link_to 'Show', book_path(@book.id) %> | <%= link_to 'Back', books_path %>
  </div>


<% end %>

